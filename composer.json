{
    "name": "suspended/quick-order",
    "description": "WooCommerce Quick Order plugin",
    "type": "wordpress-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.5",
        "wp-phpunit/wp-phpunit": "^6.6",
        "yoast/phpunit-polyfills": "^2.0",
        "nyholm/psr7": "^1.5"
    },
    "suggest": {
        "recca0120/wp-reverse-proxy": "Required for middleware integration"
    },
    "autoload": {
        "psr-4": {
            "Suspended\\QuickOrder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Suspended\\QuickOrder\\Tests\\": "tests/",
            "Recca0120\\ReverseProxy\\": "../reverse-proxy/src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
